<%- model_class = User -%>
<h2>Users</h2>

<table class="table">
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Email</th>
    <th>Role</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= link_to user.full_name, user %></td>
    <td><%= user.email %></td>
    <td><%= user.role %></td>
  </tr>
  <% end %>
</table>