<h1>Listing users</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
	 <td><%= user.first_name + " " + user.last_name %></td>
        <td>| <%= link_to 'See profile', user %> | </td>
        <% if user.id == current_user.id %>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
